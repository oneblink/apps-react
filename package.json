  "scripts": {
    "build": "tsc && npm run build:css",
    "build:css": "sass ./src/styles.scss ./dist/styles.css --load-path ./node_modules && npm run copy:sass && npm run copy:styles",
    "copy:sass": "copyfiles -u 1 ./src/styles.scss ./dist",
    "copy:styles": "copyfiles -u 1 ./src/styles/* ./dist",
    "eslint": "eslint src/**/*.{ts,tsx} --fix --cache",
    "fixpack": "fixpack",
    "jest": "jest",
    "prepare": "npm run build",
    "pretest": "npm run fixpack && npm run eslint && npm run build",
    "release": "oneblink-release",
    "start": "tsc-watch",
    "test": "npm run jest",
    "typescript": "tsc --noEmit"
